        <h1>Objects in HTML</h1>
        <div id = 'result'></div>
        <div id = 'result1'></div>
        <div id = 'result2'></div>
        <div id = 'result3'></div>
        <script>
           
            document.getElementById("result").innerHTML = "The absolute value of 20 is "+  Math.abs(-20)
            document.getElementById("result1").innerHTML = "The aSin value of -1 is "+  Math.asin(-1)
            document.getElementById("result2").innerHTML = "The aCos value of 1 is "+  Math.acos(1)
            document.getElementById("result3").innerHTML = "The atan value of 0 is "+  Math.atan(0)
            </script>
